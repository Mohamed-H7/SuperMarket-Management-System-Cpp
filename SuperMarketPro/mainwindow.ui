    height: 20px; 
    border-radius: 10px;
    border: 1px solid black; 
	background-color:white; 
}

QRadioButton::indicator:hover {
    border-color: gray; 
	background-color: gray;
}

QRadioButton::indicator:checked {
    background-color: black; 
    border-color: black; 
}

QRadioButton::indicator:checked::before {
    width: 12px;
    height: 12px;

    border-radius: 6px;
    
    position: absolute;
    top: 4px;
    left: 4px;
}
/*----------------------------*/
QPushButton {
	background-color: black;
    color: white; 
    border: 2px solid rgb(0, 0, 0);
    border-radius: 10px; 
    padding: 7px 8px; 
    font-size: 16px; 
    font-weight: bold;
}

QPushButton:hover {
	background-color: white; 
    border-color: black;
	color: black;
}

QPushButton:pressed {
    background-color:   black; 
    border-color: white;
	color: white; 
}

QPushButton:disabled {
    background-color: #bdc3c7;
    color: #7f8c8d;
    border-color: #bdc3c7; 
}

QLineEdit {
    background-color: #ecf0f1; 
    color: #2c3e50; 
    border: 2px solid #bdc3c7; 
    border-radius: 10px; 
    padding: 7px 8px; 
    font-size: 14px; 
}

QLineEdit:focus {
    border-color: #2c3e50; 
    background-color: #ffffff; 
}

QLineEdit:hover {
    border-color: #34495e; 
}

QLineEdit:disabled {
    background-color: #ecf0f1; /* لون الخلفية عندما يكون الحقل معطلاً */
    color: #95a5a6; /* لون النص عندما يكون الحقل معطلاً */
    border-color: #bdc3c7; /* لون الحدود عندما يكون الحقل معطلاً */
}

