                  <property name="styleSheet">
                   <string notr="true">QTableView {
    background-color: white; /* لون الخلفية العام للجدول */
	alternate-background-color: rgb(207, 207, 207);
    border: 1px solid black; /* لون الحدود الخارجية للجدول */
    gridline-color: black; /* لون خطوط الشبكة بين الخلايا */
    color: black; /* لون النص */
    font-size: 16px; /* حجم الخط */
    selection-background-color: black; /* لون الخلفية عند تحديد خلية */
    selection-color: white; /* لون النص عند تحديد خلية */
}

QTableView::item {
    padding: 10px; /* الحشوة الداخلية للخلايا */
    background-color: #F4F9FA; /* لون خلفية الخلايا */
    color: black; /* لون النص في الخلايا */
}

QTableView::item:selected {
    background-color: black; /* لون الخلفية عند تحديد خلية */
    color: #ffffff; /* لون النص عند تحديد خلية */
}

QTableView::item:hover {
    background-color: #1abc9c; /* لون الخلفية عند التمرير فوق خلية */
    color: #ffffff; /* لون النص عند التمرير فوق خلية */
}

QHeaderView::section {
    background-color: black; /* لون خلفية رؤوس الأعمدة */
    color: white; /* لون النص في رؤوس الأعمدة */
    padding: 5px;
    border: 1px solid black; /* لون الحدود حول رؤوس الأعمدة */
    font-size: 14px; /* حجم الخط لرؤوس الأعمدة */
	font-weight: bold;
}

QTableCornerButton::section {
    background-color: black; /* لون خلفية الزر في زاوية الجدول */
    border: 1px solid black; /* لون الحدود حول زر الزاوية */
}

QScrollBar:vertical {
    background-color:  #F4F9FA; /* لون الخلفية للشريط العمودي */
    width: 16px; /* عرض الشريط */
    margin: 20px 0 20px 0; /* المسافات العلوية والسفلية للشريط */
}

QScrollBar::handle:vertical {
    background-color: black; /* لون مقبض الشريط */
    min-height: 20px; /* أقل ارتفاع للمقبض */
    border-radius: 5px; /* زوايا دائرية للمقبض */
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    background-color: black;
    height: 20px;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:hover,
QScrollBar::sub-line:vertical:hover {
    background-color: gray; /* تغيير لون أزرار الشريط عند التمرير */
}


QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    border: none;
    width: 16px;
    height: 16px;
    background-color: transparent;
}
